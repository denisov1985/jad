    <!-- Schedules start here -->
    <section id="schedule" class="schedules schedules-seven padding-120">
        <div class="container">
            <div class="section-header text-center">
                <h3>Event Schedules</h3>
                <p><em>Quickly harness dynamic thinking through value added models.</em></p>
            </div>

            <div class="schedule-tabs">

                <!-- Nav tabs -->
                <ul class="nav nav-tabs" role="tablist">
                    {% for day in event.days %}
                    <li role="presentation" {% if (loop.index == 1) %}class="active"{% endif %}><a href="#day{{ day.id }}" aria-controls="day{{ day.id }}" role="tab"
                                                              data-toggle="tab">
                            {{ day.name }}
                            <span>{{ day.startedAt.format('j F Y') }}</span>
                        </a></li>
                    {% endfor %}
                </ul>

                <!-- Tab panes -->
                <div class="tab-content">
                    {% for day in event.days %}
                    <div role="tabpanel" class="tab-pane {% if (loop.index == 1) %}active{% endif %}" id="day{{ day.id }}">
                        <div class="schedule-table table-responsive">
                            <table>
                                <thead>
                                <tr>
                                    <th class="time">Time</th>
                                    <th style="width: 36% !important;" class="session">Session</th>
                                    <th class="spekers">Spekers</th>
                                    <th class="venue">Venue</th>
                                </tr>
                                </thead>
                                <tbody>
                                {% for activity in day.activities %}
                                <tr {%  if activity.type.id == 4 %}style="background-color: rgba(77,229,245,0.12)"{% endif %} {%  if activity.type.id == 3 %}style="background-color: #f5b4291f"{% endif %} {%  if activity.type.id == 1 %}style="background-color: #96f5291f"{% endif %}>
                                    <td class="time"><span><i class="fa fa-clock-o" aria-hidden="true"></i></span>{{ activity.startedAt.format('H:i') }}-{{ activity.endedAt.format('H:i') }}
                                    </td>
                                    <td class="session"><span><i class="fa fa-list-alt" aria-hidden="true"></i></span>{{ activity.name }}</td>
                                    <td class="spekers">
                                        {% if activity.hasSpeaker() %}
                                        <span><img style="mix-blend-mode: multiply; float: left; width: 45px; height: auto" src="/images/speakers/{{ activity.speaker.photo }}"
                                                   alt="speaker image" class="img-responsive"></span><span style="line-height: 45px">{{ activity.speaker }}</span>
                                        {% endif %}
                                    </td>
                                    <td class="venue"><span><i class="fa fa-home" aria-hidden="true"></i></span>{{ activity.room }}</td>
                                </tr>
                                {% endfor %}
                                </tbody>
                            </table>
                        </div>
                        <!-- schedule-table -->
                        <a href="#" class="schedule-button default-button">download schedule <span><i
                                        class="fa fa-download" aria-hidden="true"></i></span></a>

                    </div>
                    {% endfor %}
                </div>
            </div>
        </div>
        <!-- container -->
    </section>
